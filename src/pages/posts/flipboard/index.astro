---
// This code is identical between the lab index.astro files, can we de-dupe?

import Layout from "$layouts/Lab.astro";
import Lab from "./_Lab.svelte";
import { getEntry } from "astro:content";

const lab = await getEntry("labs", "flipboard");

if (!lab) {
  return Astro.redirect("/404");
}
---

<Layout {lab}>
  <Lab client:only />
</Layout>
